# Whether to open mock
VITE_USE_MOCK = true

# public path
# 资源公共路径,需要以 / 开头和 / 结尾
VITE_PUBLIC_PATH = /device/

# Cross-domain proxy, you can configure multiple
# Please note that no line breaks
  VITE_PROXY = [["/api","http://hs-dev.ts-it.sd.cn/device/api/"],["/upload","http://hs-dev.ts-it.sd.cn/device/api/system/file/uploadFile"]]
# VITE_PROXY = [["/api","http://device-test.tj-it.com.cn/device/api/"],["/upload","http://device-test.tj-it.com.cn/device/api/system/file/uploadFile"]]
# VITE_PROXY=[["/api","https://vvbin.cn/test"]]

# Delete console
VITE_DROP_CONSOLE = false

# Basic interface address SPA
VITE_GLOB_API_URL=/api

# File upload address， optional
VITE_GLOB_UPLOAD_URL=/upload

# Interface prefix
VITE_GLOB_API_URL_PREFIX=

# 是否通过用户中心登录
VITE_GLOB_USE_USER_CENTER_LOGIN = false
